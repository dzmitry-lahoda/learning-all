<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Itransition.Training.Data.Tag, Itransition.Training.Data" table="tags">
		<id name="Id" column="id" type="int">
			<generator class="increment" />
		</id>
    <property name="Title" column="title" type="String" index="tag_Title" />
    <bag name="Articles" table="articles_tags" inverse="true">
      <key column="Tag_Id" />
      <many-to-many class="Itransition.Training.Data.Article, Itransition.Training.Data" column="article_id" />
    </bag>
	</class>
</hibernate-mapping>